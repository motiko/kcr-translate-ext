{
  "name": "Kindle Cloud Reader Translate",
  "short_name": "KCR Translate",
  "author": "Moti Korets",
  "key": "",
  "web_accessible_resources": ["ocr.html"],
  "icons": {
    "16": "img/book_16.png",
    "128": "img/book_128.png"
  },
  "page_action": {
    "default_icon": "img/book_16.png",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
